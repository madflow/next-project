{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "passThroughEnv": ["NODE_ENV", "SENTRY_AUTH_TOKEN"]
    },
    "check-translations": {
      "dependsOn": ["^check-translations"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "e2e": {
      "cache": false
    },
    "e2e:deps": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "seed": {
      "cache": false,
      "dependsOn": ["db:migrate"]
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "stub": {
      "cache": false
    },
    "test": {
      "cache": false
    }
  },
  "ui": "stream"
}
