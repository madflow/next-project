{
  "name": "@repo/overrides",
  "version": "0.0.1",
  "description": "Overrides small npm packages",
  "scripts": {
    "build": "unbuild --clean",
    "stub": "unbuild --stub",
    "check-types": "tsc --noEmit",
    "lint": "eslint ."
  },
  "exports": {
    "./clsx": {
      "import": {
        "default": "./dist/clsx/index.mjs",
        "type": "./dist/clsx/index.d.ts"
      },
      "require": {
        "default": "./dist/clsx/index.cjs",
        "type": "./dist/clsx/index.d.cts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "devDependencies": {
    "typescript": "^5.8.3",
    "unbuild": "^3.6.0"
  }
}
